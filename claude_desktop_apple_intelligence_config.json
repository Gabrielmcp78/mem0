{
  "mcpServers": {
    "apple-intelligence-mem0": {
      "command": "node",
      "args": ["/Volumes/Ready500/DEVELOPMENT/mem0/integrations/mcp/mem0_apple_intelligence_server.js"],
      "env": {
        "PYTHONPATH": "/Volumes/Ready500/DEVELOPMENT/mem0"
      },
      "disabled": false,
      "alwaysAllow": [
        "test_connection",
        "add_memory",
        "search_memories", 
        "get_all_memories",
        "get_memory_history"
      ],
      "timeout": 60000
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/Volumes/Ready500", "/Users/gabrielmcp"]
    },
    "apple-mcp": {
      "command": "bunx",
      "args": ["--no-cache", "apple-mcp@latest"]
    },
    "applescript-execute": {
      "command": "npx",
      "args": ["@peakmojo/applescript-mcp"]
    },
    "desktop-commander": {
      "command": "npx",
      "args": ["@wonderwhy-er/desktop-commander@latest"]
    }
  }
}