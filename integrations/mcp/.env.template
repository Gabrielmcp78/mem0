# Mem0 Full Stack Configuration Template
# Copy this to .env and fill in your values

# Required: Path to your mem0 project
MEM0_PROJECT_PATH=/path/to/your/mem0/project

# Optional: Python executable path
PYTHON_PATH=/usr/local/bin/python3

# Optional: Qdrant configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=gabriel_memories

# Optional: Neo4j configuration
NEO4J_URL=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_secure_password

# Optional: Configuration file path
MEM0_CONFIG_PATH=./mem0_config.json